
class Solution {
    public int sumNumbers(TreeNode root) {
        return Sum(root,0);
        
    }
    public int Sum(TreeNode root,int sum)
    {
        if(root==null)
        {
            return 0;
        }
        if(root.left==null && root.right==null)
        {
            return sum*10+root.val;
        }
        int l=Sum(root.left,sum*10+root.val);
        
        int r=Sum(root.right,sum*10+root.val); 
        return r+l;
    }
}
