class Solution {
    public int maxProfit(int[] arr) {
        int f=0,min=arr[0],p=0;
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<min)
            {
                min=arr[i];
                 
            }
            else if(arr[i]-min>p)
            {
                p=arr[i]-min;
            }
        }
        
        return p;
    }
}
